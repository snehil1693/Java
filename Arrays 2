import java.util.*;

public class Arrays {

    public static void main(String[] args){
        int[] marks = new int[3];
        marks[0] = 97; //phy
        marks[1] = 98; //chem
        marks[2] = 95; //english

        for(int i =0; i<3; i++){
            System.out.println(marks[i]);
        }
    }
}
